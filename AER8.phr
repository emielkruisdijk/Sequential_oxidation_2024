RATES
Amm_ox
10  Fe_di = tot("Fe_di")
15  NH4 = tot("Amm")
20  O2 = mol("O2")
40  if (O2 <= 0) then goto 200
50  if (NH4 <= 0) then goto 200
60  rate = parm(1)*NH4
70  moles = rate*time
80  if (moles > NH4) then moles = NH4
85  if (moles > 2*O2) then moles = O2/2
86  rate = moles/time
90  put(rate,20)
95  put(moles, 200)
200 SAVE moles
-end

Mn_di_hetero #Davies and Morgan, 1989
-start
5 Mn_di = MOL("Mn_di+2")
10 if (Mn_di <= 0) then goto 100
20 Mn_di_ads =  parm(1)
30 p_o2 = 10^(SI("O2(g)"))
40 rate = parm(2) * Mn_di_ads * p_o2
50 moles = rate * time
60 put(rate,5)
70 if (moles > Mn_di) then moles = Mn_di
75 put(moles, 50)
100 save moles
-end

